tc 1
#Headset-mic to Speaker Dual-Channel AFE loopback
!AfeLoop
Txdevice:11
Rxdevice:5

enable
ADC2 Volume:5
DEC1 MUX:ADC2
MICBIAS CAPLESS Switch:1
ADC2 MUX:INP2
IIR1 INP1 MUX:DEC1

RX1 MIX1 INP1:RX1
RX2 MIX1 INP1:RX2
RDAC2 MUX:RX2
HPHL:Switch
HPHR:Switch
Ext Spk Switch:On
RX1 Digital Volume:77
RX2 Digital Volume:77

PRI_MI2S_RX Port Mixer TERT_MI2S_TX:1

disable
PRI_MI2S_RX Port Mixer TERT_MI2S_TX:0

ADC2 Volume:4
DEC1 MUX:ZERO
MICBIAS CAPLESS Switch:0
ADC2 MUX:ZERO
IIR1 INP1 MUX:ZERO

RX1 MIX1 INP1:ZERO
RX2 MIX1 INP1:ZERO
RDAC2 MUX:ZERO
HPHL:ZERO
HPHR:ZERO
Ext Spk Switch:Off
RX1 Digital Volume:84
RX2 Digital Volume:84




